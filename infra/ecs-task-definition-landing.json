{
  "family": "default-landing-cashsouk-1156",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::652821469470:role/service-role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::652821469470:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "Main",
      "image": "ACCOUNT_ID.dkr.ecr.ap-southeast-5.amazonaws.com/cashsouk-landing:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "PORT",
          "value": "3000"
        },
        {
          "name": "NEXT_PUBLIC_COGNITO_DOMAIN",
          "value": "auth.cashsouk.com"
        },
        {
          "name": "NEXT_PUBLIC_INVESTOR_URL",
          "value": "https://investor.cashsouk.com"
        },
        {
          "name": "NEXT_PUBLIC_ISSUER_URL",
          "value": "https://issuer.cashsouk.com"
        },
        {
          "name": "NEXT_PUBLIC_ADMIN_URL",
          "value": "https://admin.cashsouk.com"
        },
        {
          "name": "NEXT_PUBLIC_LANDING_URL",
          "value": "https://www.cashsouk.com"
        },
        {
          "name": "NEXT_PUBLIC_COOKIE_DOMAIN",
          "value": ".cashsouk.com"
        }
      ],
      "secrets": [
        {
          "name": "NEXT_PUBLIC_COGNITO_USER_POOL_ID",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:Frontend_Secrets-eB5XhF:NEXT_PUBLIC_COGNITO_USER_POOL_ID::"
        },
        {
          "name": "NEXT_PUBLIC_COGNITO_CLIENT_ID",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:Frontend_Secrets-eB5XhF:NEXT_PUBLIC_COGNITO_CLIENT_ID::"
        },
        {
          "name": "NEXT_PUBLIC_COGNITO_REGION",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:Frontend_Secrets-eB5XhF:NEXT_PUBLIC_COGNITO_REGION::"
        },
        {
          "name": "NEXT_PUBLIC_API_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:Frontend_Secrets-eB5XhF:NEXT_PUBLIC_API_URL::"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/cashsouk-landing",
          "awslogs-region": "ap-southeast-5",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
