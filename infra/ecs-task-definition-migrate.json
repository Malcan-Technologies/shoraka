{
  "family": "cashsouk-migrate",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::652821469470:role/service-role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::652821469470:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "migrate",
      "image": "652821469470.dkr.ecr.ap-southeast-5.amazonaws.com/cashsouk-migrate:latest",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/cashsouk-migrate",
          "awslogs-region": "ap-southeast-5",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      },
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        }
      ],
      "secrets": [
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:cashsouk/database-url"
        }
      ]
    }
  ]
}
