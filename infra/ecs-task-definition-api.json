{
  "family": "default-api-cashsouk-09ff",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::652821469470:role/service-role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::652821469470:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "Main",
      "image": "652821469470.dkr.ecr.ap-southeast-5.amazonaws.com/api-cashsouk:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 4000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/cashsouk-api",
          "awslogs-region": "ap-southeast-5",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "PORT",
          "value": "4000"
        },
        {
          "name": "ALLOWED_ORIGINS",
          "value": "https://cashsouk.com,https://investor.cashsouk.com,https://issuer.cashsouk.com,https://admin.cashsouk.com"
        },
		{
			"name": "EMAIL_FROM",
			"value": "no-reply@cashsouk.com"
		}
      ],
      "secrets": [
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:cashsouk/database-url-rsKFxD"
        },
        {
          "name": "COGNITO_USER_POOL_ID",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:COGNITO_USER_POOL_ID::"
        },
        {
          "name": "COGNITO_CLIENT_ID",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:COGNITO_CLIENT_ID::"
        },
        {
          "name": "COGNITO_CLIENT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:COGNITO_CLIENT_SECRET::"
        },
        {
          "name": "COGNITO_DOMAIN",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:COGNITO_DOMAIN::"
        },
        {
          "name": "COGNITO_REGION",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:COGNITO_REGION::"
        },
        {
          "name": "REDIRECT_URI",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:REDIRECT_URI::"
        },
        {
          "name": "SESSION_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:SESSION_SECRET::"
        },
        {
          "name": "FRONTEND_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:FRONTEND_URL::"
        },
        {
          "name": "INVESTOR_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:INVESTOR_URL::"
        },
        {
          "name": "ISSUER_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:ISSUER_URL::"
        },
        {
          "name": "ADMIN_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:ADMIN_URL::"
        },
        {
          "name": "COOKIE_DOMAIN",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:COOKIE_DOMAIN::"
        },
		{
			"name": "SES_REGION",
			"valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:SES_REGION::"
		},
		{
			"name": "API_URL",
			"valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:API_URL::"
		},
		{
			"name": "DATABASE_URL_DEV",
			"valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:DATABASE_URL_DEV::"
		},
		{
			"name": "REGTANK_OAUTH_URL",
			"valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:REGTANK_OAUTH_URL::"
		},
		{
			"name": "REGTANK_API_BASE_URL",
			"valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:REGTANK_API_BASE_URL::"
		},
		{
			"name": "REGTANK_ONBOARDING_PROXY_URL",
			"valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:REGTANK_ONBOARDING_PROXY_URL::"
		},
		{
			"name": "REGTANK_CLIENT_ID",
			"valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:REGTANK_CLIENT_ID::"
		},
		{
			"name": "REGTANK_CLIENT_SECRET",
			"valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:REGTANK_CLIENT_SECRET::"
		},
		{
			"name": "REGTANK_WEBHOOK_SECRET",
			"valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:REGTANK_WEBHOOK_SECRET::"
		},
		{
			"name": "REGTANK_REDIRECT_URL_INVESTOR",
			"valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:REGTANK_REDIRECT_URL_INVESTOR::"
		},
		{
			"name": "REGTANK_REDIRECT_URL_ISSUER",
			"valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:REGTANK_REDIRECT_URL_ISSUER::"
		},
		{
			"name": "REGTANK_INVESTOR_PERSONAL_FORM_ID",
			"valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:REGTANK_INVESTOR_PERSONAL_FORM_ID::"
		},
		{
			"name": "REGTANK_INVESTOR_CORPORATE_FORM_ID",
			"valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:REGTANK_INVESTOR_CORPORATE_FORM_ID::"
		},
		{
			"name": "REGTANK_ISSUER_CORPORATE_FORM_ID",
			"valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:REGTANK_ISSUER_CORPORATE_FORM_ID::"
		},
		{
			"name": "REGTANK_INVESTOR_CORPORATE_FORM_NAME",
			"valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:REGTANK_INVESTOR_CORPORATE_FORM_NAME::"
		},
		{
			"name": "REGTANK_ISSUER_CORPORATE_FORM_NAME",
			"valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:REGTANK_ISSUER_CORPORATE_FORM_NAME::"
		},
		{
			"name": "REGTANK_WEBHOOK_MODE",
			"valueFrom": "arn:aws:secretsmanager:ap-southeast-5:652821469470:secret:BACKEND_ENV-xymJJS:REGTANK_WEBHOOK_MODE::"
		}
      ],
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "node -e \"require('http').get('http://localhost:4000/healthz', (r) => process.exit(r.statusCode === 200 ? 0 : 1))\""
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      }
    }
  ]
}
